-        local i j
-
-        # repeatable flags with arguments, where the values can't contain colons
-        for i in ${(k)opt_args[(I)(-a|--architecture)]}; do
-          for j in ${(@s.:.)opt_args[$i]}; do
-            [[ -n $j ]] || continue
