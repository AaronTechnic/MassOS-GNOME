-      local -a versions=( ${(Oaf)"$(_call_program versions debsnap --list ${args} -- ${line[1]})"} )
-      _description -V versions expl ${state_descr}
-      compadd "$@" "${expl[@]}" -a versions && ret=0
-    else
-      _message -- $state_descr && ret=0
-    fi
-    ;;
-esac
-
-return ret
